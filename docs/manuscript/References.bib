@article{fengRecentChangesArctic2021b,
  title = {Recent Changes to {{Arctic}} River Discharge},
  author = {Feng, Dongmei and Gleason, Colin J. and Lin, Peirong and Yang, Xiao and Pan, Ming and Ishitsuka, Yuta},
  year = {2021},
  month = nov,
  journal = {Nature Communications},
