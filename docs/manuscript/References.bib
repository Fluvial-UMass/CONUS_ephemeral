@article{schmadelSmallPondsHeadwater2019,
  title = {Small {{Ponds}} in {{Headwater Catchments Are}} a {{Dominant Influence}} on {{Regional Nutrient}} and {{Sediment Budgets}}},
  author = {Schmadel, Noah M. and Harvey, Judson W. and Schwarz, Gregory E. and Alexander, Richard B. and Gomez-Velez, Jesus D. and Scott, Durelle and Ator, Scott W.},
  year = {2019},
  journal = {Geophysical Research Letters},
  volume = {46},
  number = {16},
  pages = {9669--9677},
  issn = {1944-8007},
  doi = {10.1029/2019GL083937},
  abstract = {Small ponds\textemdash farm ponds, detention ponds, or impoundments below 0.01 km2\textemdash serve important human needs throughout most large river basins. Yet the role of small ponds in regional nutrient and sediment budgets is essentially unknown, currently making it impossible to evaluate their management potential to achieve water quality objectives. Here we used new hydrography data sets and found that small ponds, depending on their spatial position within both their local catchments and the larger river network, can dominate the retention of nitrogen, phosphorus, and sediment compared to rivers, lakes, and reservoirs. Over 300,000 small ponds are collectively responsible for 34\%, 69\%, and 12\% of the mean annual retention of nitrogen, phosphorus, and sediment in the Northeastern United States, respectively, with a dominant influence in headwater catchments (54\%, 85\%, and 50\%, respectively). Small ponds play a critical role among the many aquatic features in long-term nutrient and sediment loading to downstream waters.},
  langid = {english},
  keywords = {ponded waters,regional nutrient and sediment budgets,river corridor,small ponds},
  annotation = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2019GL083937},
  file = {C\:\\Users\\craig\\Zotero\\storage\\RKWWMEJM\\Schmadel et al. - 2019 - Small Ponds in Headwater Catchments Are a Dominant.pdf;C\:\\Users\\craig\\Zotero\\storage\\3LPW3MII\\2019GL083937.html}
}

@article{schmidtExportPlasticDebris2017,
  title = {Export of {{Plastic Debris}} by {{Rivers}} into the {{Sea}}},
  author = {Schmidt, Christian and Krauth, Tobias and Wagner, Stephan},
  year = {2017},
